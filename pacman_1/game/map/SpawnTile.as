package game.map {	import game.objects.GameObject;		public class SpawnTile extends Tile{		protected var spawnObject:GameObject;			public function SpawnTile(spawn:GameObject, solid:Boolean, slope:int, top:Boolean) {			// constructor code			this.spawnObject = spawn;			super(solid, slope, top);		}				public function spawn(count:int):Array {			var spawn_array:Array= [];						for( var i:int=0; i<count;i++){				var new_spawn = this.spawnObject.clone();				spawn_array.push(new_spawn);							if (this.stage != undefined || this.stage != null){					// add enemy to game client					this.root.gameClient.addEnemy(new_spawn, x, y, dx, dy, false);				}			}						return spawn_array;		}	}	}