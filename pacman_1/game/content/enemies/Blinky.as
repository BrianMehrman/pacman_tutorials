package game.content.enemies {	import game.objects.Enemy;	import game.objects.MovementHandler;	import game.objects.GameObject;	import flash.events.Event;	import flash.utils.Timer;	import flash.events.TimerEvent;		public class Blinky extends Enemy {		public function Blinky(xTile:uint, yTile:uint, dirX:int, dirY:int, clientHero:Boolean, initTarget:GameObject, movementHandler:MovementHandler):void {						this.addEventListener(Event.ADDED_TO_STAGE, startTimer);						super(24, 24, xTile, yTile, dirX, dirY, 5, 10, 2, clientHero, initTarget, movementHandler);		}				public function startTimer(e:Event):void {			var timer:Timer = new Timer(2000,1);			timer.addEventListener(TimerEvent.TIMER, startMovement)			timer.start();		}				public function startMovement(e:TimerEvent):void {			this.canMove = true;		}	}	}