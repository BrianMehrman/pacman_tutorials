package game.ai.pathfinding {		import game.Game;		public interface IPathfinder {		/**		 * 		 * Finds the path.		 * 		 * @param curX The current x tile coordinate.		 * @param curY The current y tile coordinate.		 * @param destX The destination x tile coordinate.		 * @param destY The destination y tile coordinate.		 * @param gameTiles The <code>Object</code> containing the x, y coordinate pairs and associated Tile objects		 * of all the game's tiles.		 * 		 * @return An <code>Vector.<MapNode></code> containing the path from the starting <code>MapNode</code> to the ending <code>MapNode</code>.		 * 		 */				function findPath(curX:uint, curY:uint, destX:uint, destY:uint, gameTiles:Object):Vector.<MapNode>;				/**		 * 		 * Gets the corners from a path.		 * 		 * @param path The path containing the <code>MapNodes</code>s.		 * 		 * @return An <code>Vector.<MapNode><code> containing the corners in the path.		 * 		 */				function getCorners(path:Vector.<MapNode>):Vector.<MapNode>;	}	}