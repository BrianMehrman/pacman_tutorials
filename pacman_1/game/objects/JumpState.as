package game.objects {		public class JumpState {						public var distanceFromGround:int;				public var currentJumpVelocity:int;		public var currentForwardVelocity:int;		public var jumpMoveDirX:int;				public var jumpMoveDirY:int;				public var jumpDirX:int;				public var jumpDirY:int;				/**		 *		 * @param distanceFromGround The distance from the ground in pixels.		 * @param currentJumpVelocity The current jump velocity.		 * @param currentForwardVelocity The current forward velocity;		 * @param jumpMoveDirX The x movement direction of the jump.		 * @param jumpMoveDirY The y movement direction of the jump.		 * @param jumpDirX The x facing direction of the jump.		 * @param jumpDirY The y facing direction of the jump.		 *		 */		public function JumpState(distanceFromGround:int=0, currentJumpVelocity:int=1, currentForwardVelocity:int = 1, jumpMoveDirX:int=0, jumpMoveDirY:int=0, jumpDirX:int=0, jumpDirY:int=0) {			// constructor code			this.distanceFromGround = distanceFromGround;			this.currentJumpVelocity = currentJumpVelocity;			this.currentForwardVelocity = currentForwardVelocity;			this.jumpDirX = jumpDirX;			this.jumpDirY = jumpDirY;			this.jumpMoveDirX = jumpMoveDirX;			this.jumpMoveDirY = jumpMoveDirY;		}	}	}