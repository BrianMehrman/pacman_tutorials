package game.objects {	import flash.display.MovieClip;		public class EnemyHandler {				protected var _enemies:Array = new Array();		protected var _tilesContainer:MovieClip;				public function EnemyHandler(tilesContainer:MovieClip) {			// constructor code			this._tilesContainer = tilesContainer;		}		public function addEnemy(enemy:Enemy):void {			this._enemies.push(enemy);			this._tilesContainer.addChild(enemy);		}	}	}