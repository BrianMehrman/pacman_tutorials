package game.content.tiles {	import game.map.Tile;	import flash.events.Event;	import game.objects.Pellet;	public class OpenTile extends Tile {		public function OpenTile():void {			this.addEventListener(Event.ADDED_TO_STAGE, initTile);			super(false, 0, false);		}				public function initTile(e:Event):void 		{			var xTile:int = Math.ceil(this.x/this.width);			var yTile:int = Math.ceil(this.y/this.height);			var p:Pellet = new Pellet(16, 16, xTile, yTile, this.game);			p.x = this.x;			p.y = this.y;			this.game.addGameObject(p);			e.currentTarget.parent.addChild(p);		}	}}