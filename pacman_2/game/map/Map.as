package game.map {	/*	 *	 */	public class Map {		public var mapID:Number;		public var mapName:String;		public var tileset:Array;		public var tilespawns:Array;		public var mapArray:Array;						/*			@param mapID The ID of the map.			@param mapName The name of the map.			@param tileset The tile set of the map.			@param mapArray The array containing the map.		*/		public function Map(mapID:Number, mapName:String, tileset:Array, tilespawns:Array, mapArray:Array):void {			// constructor code			this.mapID = mapID;			this.mapName = mapName;			this.tileset = tileset;			this.tilespawns = tilespawns;			this.mapArray = mapArray;		}	}	}