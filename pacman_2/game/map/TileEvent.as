package game.map {	import flash.events.Event;		public class TileEvent extends Event {				public static const SPAWN_OBJECT:String  = "spawnObject";		public var spawn_tile:SpawnTile;		public function TileEvent(type:String, spawn_tile:SpawnTile, bubbles:Boolean=false, cancelable:Boolean=false) {			// constructor code			this.spawn_tile = spawn_tile;			super(type,bubbles,cancelable);		}				public override function clone():Event {			return new  TileEvent(type, spawn_tile, bubbles, cancelable);		}		public override function toString():String {			return formatToString("TileEvent","type","spawn_tile","bubble","cancelable");		}	}	}