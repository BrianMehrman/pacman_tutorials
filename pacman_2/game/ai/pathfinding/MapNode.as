package game.ai.pathfinding {		public class MapNode {		public var xCoord:int;		public var yCoord:int;		public var pathToNode:Vector.<MapNode>;				/**		 * 		 * A map node used for pathfinding.		 * 		 * @param xCoord The x tile coordinate of the node.		 * @param yCoord The y tile coordinate of the node.		 * @param pathToNode An <code>Vector.<MapNode></code> containing MapNodes that in succession lead to this node.		 * 		 */				public function MapNode(xCoord:int, yCoord:int, pathToNode:Vector.<MapNode>) {						this.xCoord = xCoord;			this.yCoord = yCoord;			this.pathToNode = pathToNode;					}	}	}