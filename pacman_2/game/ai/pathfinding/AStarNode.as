package game.ai.pathfinding {		public class AStarNode extends MapNode {				/**		 * 		 * The movement cost to the node.		 * 		 */				public var g:Number;				/**		 * 		 * The movement cost from the node to the target node using an heuristic.		 * 		 */				public var h:Number;				/**		 * 		 * An A* node.		 * 		 * @param xCoord The x tile coordinate of the node.		 * @param yCoord The y tile coordinate of the node.		 * @param pathToNode An <code>Vector.<MapNode></code> containing MapNodes that in succession lead to this node.		 * @param g The calculated G value (movement cost from the beginning node to this node).		 * @param h The calcluated H value (the heuristic cost from this node to the end node).		 * 		 */				public function AStarNode(xCoord:int, yCoord:int, pathToNode:Vector.<MapNode>, g:Number, h:Number) {			this.g = g;			this.h = h;						super(xCoord, yCoord, pathToNode);		}	}}