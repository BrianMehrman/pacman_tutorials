package game.controls {		import flash.events.Event;		public class KeyEvent extends Event{				public static const KEYS_DOWN = "keysDown";				public static const NO_KEY_DOWN = "noKeyDown";				public var keys:Object;				public function KeyEvent(type:String, keys:Object=null, bubbles:Boolean=false, cancelable:Boolean=false) {			// constructor code			super(type,bubbles,cancelable);			this.keys = keys;		}	}	}