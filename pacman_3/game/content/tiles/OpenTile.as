package game.content.tiles {	import game.map.Tile;	import flash.events.Event;	import game.objects.Pellet;	import game.map.SpawnTile;	import game.GameClient;	public class OpenTile extends SpawnTile {		public function OpenTile():void {			//this.addEventListener(Event.ADDED_TO_STAGE, initTile);						super(Pellet,0,false, 0, false);		}				override public function spawnProxy(gameClient:GameClient,xTile:int, yTile:int):void {						var p:Pellet = new Pellet(16, 16, xTile, yTile, this.game);			gameClient.addGameObject(p);		}					}}