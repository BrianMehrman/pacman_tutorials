package game.objects {	import flash.display.MovieClip;	import game.Game;	import game.objects.GameObject;	import game.objects.GameObjectEvent;	import flash.events.Event;		public class Item extends GameObject {		public var points:int;		public var e:Event		public function Item(objectWidth:uint, objectHeight:uint, xTile:uint, yTile:uint, game:Game):void {			// constructor code						var requiredLabels:Array = new Array();			this.points = 100;			this.clientHero = false;			this.is_vulnerable = true;			this.canMove = false;			this.lives = 1;			super(objectWidth, objectHeight, xTile, yTile, 0, 0, 0, requiredLabels, game);		}				override protected function checkLoop(e:Event):void {			//check collision			if(this.stage != null && this.hitTestObject(this._game.clientHero)){				// dispatch event				this.stage.dispatchEvent(new GameObjectEvent(GameObjectEvent.OBJECT_COLLECTED, this._game.clientHero,this));			}			// super			super.checkLoop(e);		}				// another game object has picked me up. apply points to the game objects score		public function pickedUp(collector:GameObject):void {			collector.score += this.points;		}	}	}